<ul class="login-partial small-10">
  <h3>LeagueHQ Sign In</h3>
  <%= form_for(:user, :url => session_path(:user)) do |f| %>
    <li><%= f.text_field :email, placeholder: "Email" %></li>
    <li><%= f.password_field :password, placeholder: "Password" %></li>
    <li><%= f.check_box :remember_me %> <%= f.label :remember_me, class: "user_remember_me"%></li>
    <li><%= f.submit 'Sign in' %></li>
    <li><%= link_to "Forgot your password?", new_password_path(:user) %></li>
  <% end %>
</ul>
